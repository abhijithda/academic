	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.cc.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.cxx.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.C.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.c.o:
	$(CC) -c $(CFLAGS) $(INCPATH) -o $@ $<

####### Build rules

all: Makefile $(TARGET)

$(TARGET): $(UICDECLS) $(OBJECTS) $(OBJMOC) 
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(LIBS)

mocables: $(SRCMOC)

$(MOC): 
	( cd $(QTDIR)/src/moc ; $(MAKE) )

Makefile: nmproject.pro  $(QTDIR)/mkspecs/default/qmake.conf 
	$(QMAKE) -o Makefile nmproject.pro
qmake: 
	@$(QMAKE) -o Makefile nmproject.pro

dist: 
	@mkdir -p .obj/nmproject && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .obj/nmproject/ && $(COPY_FILE) --parents nmformproject.ui.h .obj/nmproject/ && ( cd `dirname .obj/nmproject` && $(TAR) nmproject.tar nmproject && $(GZIP) nmproject.tar ) && $(MOVE) `dirname .obj/nmproject`/nmproject.tar.gz . && $(DEL_FILE) -r .obj/nmproject

mocclean:
	-$(DEL_FILE) $(OBJMOC)
	-$(DEL_FILE) $(SRCMOC)

uiclean:
	-$(DEL_FILE) $(UICIMPLS) $(UICDECLS)

yaccclean:
lexclean:
clean: mocclean uiclean
	-$(DEL_FILE) $(OBJECTS) 
	-$(DEL_FILE) *~ core *.core


####### Sub-libraries

distclean: clean
	-$(DEL_FILE) $(TARGET) $(TARGET)


FORCE:

####### Compile

.obj/main.o: main.cpp .ui/nmformproject.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/main.o main.cpp

.ui/nmformproject.h: nmformproject.ui 
	$(UIC) nmformproject.ui -o .ui/nmformproject.h

.ui/nmformproject.cpp: .ui/nmformproject.h nmformproject.ui 
	$(UIC) nmformproject.ui -i nmformproject.h -o .ui/nmformproject.cpp

.obj/nmformproject.o: .ui/nmformproject.cpp nmformproject.ui.h \
		.ui/nmformproject.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/nmformproject.o .ui/nmformproject.cpp

.obj/moc_nmformproject.o: .moc/moc_nmformproject.cpp .ui/nmformproject.h 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/moc_nmformproject.o .moc/moc_nmformproject.cpp

.moc/moc_nmformproject.cpp: $(MOC) .ui/nmformproject.h
	$(MOC) .ui/nmformproject.h -o .moc/moc_nmformproject.cpp

